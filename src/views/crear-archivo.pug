//-Layout Base
extends ../templates/base.pug

//-Etiqueta Title
block title
	title Ejemplo de meta title

//-Etiqueta Metadescription
block description
	meta(name="description", content="Frontend BRM" )


block content
	section.container-fluid
		div.row
			include ../templates/partials/menu.pug


	section.container-fluid.alto-page
		//-Nombre Usuario
		div.row
			include ../templates/partials/header-user.pug
		//-Contenedor
		div.row
			//-Titulos
			article.col-general
				div.cont-titulos-generales
					h4 Crear archivo

				//-Contenido
				div.cont-contenido-detalle
					
					//-Espacio arrastrar
					div.col-contenido-doble.cont-drag-archivo

						div.cont-texto-drag
							span.glyphicon.glyphicon-inbox
							|</br>Drop files here to upload
		
				//-Volver
		div.row
			div.cont-btn-volver
				a(href="archivos.html").btn-volver-general
					span.glyphicon.glyphicon-chevron-left
					</br>volver


